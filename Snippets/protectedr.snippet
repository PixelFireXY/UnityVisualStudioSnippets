<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>protectedr</Title>
			<Author>Matteo Vittori</Author>
			<Description>Snippet for a blanc protected method with return type.</Description>
			<Shortcut>protectedr</Shortcut>
        </Header>
        <Snippet>
            <Code Language="CSharp">
                <![CDATA[protected $Ret$ $Var$()
				{
					$selected$
					
					return null$end$;
				}]]>
            </Code>
			<Declarations>
			<Literal>
			  <ID>Var</ID>
			  <ToolTip>The name of the method.</ToolTip>
			  <Default>MethodName</Default>
			</Literal>
			<Literal>
			  <ID>Ret</ID>
			  <ToolTip>The return type of the method.</ToolTip>
			  <Default>object</Default>
			</Literal>
		  </Declarations>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>